    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "tslint-config-prettier": "^1.18.0",
    "prettier": "^2.0.5",
    "codecov": "^3.5.0",
    "ng-packagr": "^10.0.0",
    "ngx-highlight-js": "^10.0.3"
